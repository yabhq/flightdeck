{
    "name": "yabhq/flightdeck",
    "description": "API boilerplate for Laravel",
    "keywords": [
        "yab",
        "yabhq",
        "flightdeck"
    ],
    "homepage": "https://github.com/yabhq/flightdeck",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chris Blackwell",
            "email": "chris@yabhq.com",
            "role": "Developer"
        },
        {
            "name": "Jim Hlad",
            "email": "jim@yabhq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "tymon/jwt-auth": "^2.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Yab\\FlightDeck\\": "src"
        }
    },
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Yab\\FlightDeck\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yab\\FlightDeck\\FlightDeckServiceProvider"
            ]
        }
    }
}
